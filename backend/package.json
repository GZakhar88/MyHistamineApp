{
  "name": "my-histamine-app",
  "version": "1.0.0",
  "description": "histamine tracker for personal use",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll",
    "test:ci": "jest --ci --collectCoverage --reporters=default",
    "test:cover": "jest --collectCoverage",
    "start": "nodemon --experimental-modules --es-module-specifier-resolution=node --require dotenv/config src/index.js",
    "build": "babel src -d dist",
    "serve": "node -r dotenv/config dist/index.js"
  },
  "type": "module",
  "author": "Gabor Zakhar (Zaza)",
  "license": "ISC",
  "dependencies": {
    "db-migrate": "^0.11.12",
    "db-migrate-mysql": "^2.1.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.15.0",
    "babel-jest": "^27.0.6",
    "jest": "^27.0.6"
  }
}
